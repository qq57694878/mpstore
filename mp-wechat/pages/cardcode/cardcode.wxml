
<view class="container">

   <view class="my-wrap">
    <view class="my-codes-text">{{barcode}}</view>
    <view class="my-codes-wrap">
        <canvas   class="my-barcode" canvas-id="barcodeCanvas"/>

        <view style="height: 40rpx;width: 0"></view>
        <canvas  class="my-qrcode" canvas-id="qrcodeCanvas"/>
    </view>

    <view style="display: flex;flex-direction: row;justify-content: center">
        <text style="color: #333333;font-size: 30rpx;padding-right: 200rpx;padding-left: 200rpx;padding-top: 30rpx;padding-bottom: 30rpx;" bindtap="refresh">60秒自动刷新</text>
    </view>
   </view>
   



</view>


